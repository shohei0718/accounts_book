<h1>支出費目登録</h1>
  <%= form_for @item, url:outcome_items_create_path  do |f| %>
  <div class="form_group">
    <%= f.label :title, '支出費目' %>
    <%= f.text_field :title, class: 'outcome_items-form' %>
  </div>
  <div class="btn-block">
    <%= link_to 'トップページへ戻る', top_index_path, class: "btn" %>
    <%= f.submit '登録', class: "btn" %>
  </div>
  <% end %>
